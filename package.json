{
  "name": "@bit/loader",
  "version": "10.0.3",
  "description": "Framework for building module loaders with very little effort for noobs",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "module",
    "modules",
    "import",
    "loader"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "pakit src/bit-loader.js --out dist/index.js",
    "test": "npm run lint && mocha --require babel-register test/SpecRunner.js",
    "lint": "eslint src/**/*.js test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MiguelCastillo/bit-loader.git"
  },
  "author": "Miguel Castillo <manchagnu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MiguelCastillo/bit-loader/issues"
  },
  "homepage": "https://github.com/MiguelCastillo/bit-loader",
  "devDependencies": {
    "babel-bits": "^1.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bit-imports": "^3.1.1",
    "chai": "~4.1.2",
    "chance": "^1.0.16",
    "config-grunt-tasks": "^1.0.3",
    "connect-livereload": "^0.6.0",
    "eslint-tap": "^2.0.1",
    "grunt": "~1.0.3",
    "grunt-cli": "^1.3.1",
    "grunt-concurrent": "~3.0.0",
    "grunt-contrib-connect": "~2.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-mocha": "^1.1.0",
    "grunt-pakit": "^0.2.2",
    "grunt-release": "~0.14.0",
    "load-grunt-tasks": "^4.0.0",
    "mocha": "~5.2.0",
    "pakit": "^1.0.2",
    "sinon": "~6.3.3"
  },
  "dependencies": {
    "belty": "^5.2.2",
    "dis-isa": "^1.3.2",
    "loggero": "~2.1.0",
    "roolio": "~0.1.1",
    "spromise": "~1.0.0",
    "then-pipeline": "^1.0.1"
  }
